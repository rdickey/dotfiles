[push]
	default = current
[alias]
	co = checkout
	st = status
	ci = commit
	br = branch
	lg = log --pretty=format:'%Cred%h%Creset - %Cgreen%an, %ar%Creset - %s'
	cp = cherry-pick
	unpushed = !GIT_CURRENT_BRANCH=$(git name-rev --name-only HEAD) && git log origin/$GIT_CURRENT_BRANCH..$GIT_CURRENT_BRANCH --oneline
[color]
	branch = auto
	diff = auto
